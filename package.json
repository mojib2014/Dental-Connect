    "axios": "^0.16.2",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.17.2",
    "bootstrap": "^4.1.1",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^5.0.1",
    "express": "^4.15.4",
    "if-env": "^1.0.4",
    "input-moment": "^0.4.0",
    "moment": "^2.22.2",
    "mongoose": "^5.0.17",
    "mongoose-find-or-create": "^1.3.1",
    "react": "^16.4.1",
    "react-datepicker": "^1.5.0",
